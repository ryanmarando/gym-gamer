{
    "cli": {
        "version": ">= 16.14.0",
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "env": {
                "GOOGLE_SERVICES_JSON": "@GOOGLE_SERVICES_JSON",
                "GOOGLE_SERVICES_JSON_PATH": "./google-services.json"
            }
        },
        "preview": {
            "distribution": "internal",
            "env": {
                "GOOGLE_SERVICES_JSON": "@GOOGLE_SERVICES_JSON",
                "GOOGLE_SERVICES_JSON_PATH": "./google-services.json"
            }
        },
        "production": {
            "autoIncrement": true,
            "env": {
                "GOOGLE_SERVICES_JSON": "@GOOGLE_SERVICES_JSON",
                "GOOGLE_SERVICES_JSON_PATH": "./google-services.json"
            }
        }
    },
    "submit": {
        "production": {}
    }
}
